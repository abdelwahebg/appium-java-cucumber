<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TestNG Cucumber Suite">
    <parameter name="deviceName" value="API22"/>
    <parameter name="platformVersion" value="5.1.1"/>
    <parameter name="port" value="4724"/>
    <parameter name="module" value="APP"/>
    <parameter name="browser" value="CHROME"/>
    <parameter name="emulation" value="Iphone 6/7/8"/>

    <listeners>
        <listener class-name="bukalapak.utility.TestNGExecutionListener"></listener>
    </listeners>
    <test name="Run Login Test">
        <classes>
            <class name="bukalapak.cucumber.CucumberTestRunner"/>
        </classes>
    </test>
</suite>